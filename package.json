{
  "name": "super-hero-ui",
  "version": "1.0.0",
  "description": "The Example Code walked through during Weeks 14, 15, and 16. This code should be built live in class. This repo is merely for instructor/TA reference. This code represents the final version at the end of the class.",
  "main": "index.jsx",
  "scripts": {
    "build": "export VERSION=$(date +\"%Y%m%d_%H%M%S\") && npm run build:clean && npm run build:server && npm run build:client && npm run build:copy && npm run build:zip",
    "build:clean": "rm -f superhero_*.zip && rm -rf ./dist && mkdir -p ./dist/public",
    "build:client": "./node_modules/.bin/webpack --progress --config ./webpack.prod.js",
    "build:copy": "cp ./package.json ./dist",
    "build:server": "./node_modules/.bin/babel ./ --out-dir ./dist/ --ignore ./actions,./components,./node_modules,./pages,./public,./tests,./utils,./.babelrc,./.env,./.env-sample,./.eslintignore,./.eslintrc.js,./.gitignore,./client.jsx,./package-lock.json,./README.md,./webpack.dev.js,./webpack.prod.js",
    "build:zip": "cd ./dist && zip -r ../superhero_$VERSION.zip ./",
    "deploy": "export VERSION=$(date +\"%Y%m%d_%H%M%S\") && npm run build && npm run deploy:upload && npm run deploy:newversion && npm run deploy:updateenv",
    "deploy:newversion": "aws elasticbeanstalk create-application-version --application-name superhero --version-label superhero_$VERSION --source-bundle S3Bucket=elasticbeanstalk-us-east-1-227911150546,S3Key=superhero_$VERSION.zip --profile superhero-deploy",
    "deploy:updateenv": "aws elasticbeanstalk update-environment --application-name superhero --version-label superhero_$VERSION --environment-name Superhero-env-2 --profile superhero-deploy",
    "deploy:upload": "aws s3 cp superhero_$VERSION.zip s3://elasticbeanstalk-us-east-1-263608175784/superhero_$VERSION.zip --profile superhero-deploy",
    "dev": "./node_modules/.bin/concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "./node_modules/.bin/webpack --progress --inline --config webpack.dev.js",
    "dev:server": "./node_modules/.bin/env-cmd ./node_modules/.bin/babel-watch server.js",
    "dev:migrate": "./node_modules/.bin/env-cmd ./node_modules/.bin/sequelize db:migrate --env development --config ./config/sequelize.js",
    "lint": "./node_modules/.bin/eslint --format codeframe --ext .jsx .",
    "prod:migrate": "./node_modules/.bin/sequelize db:migrate --env production --config ./config/sequelize.js",
    "prod:server": "node server.js",
    "start": "npm run prod:migrate && npm run prod:server",
    "test": "./node_modules/.bin/mocha --require @babel/register --require ./tests/setup.js tests/**/*.test.js*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackeducation/super-hero-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stackeducation/super-hero-ui/issues"
  },
  "homepage": "https://github.com/stackeducation/super-hero-ui#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "axios-mock-adapter": "^1.18.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-watch": "^7.0.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "mocha": "^7.1.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
